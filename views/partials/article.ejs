<ul class="articleList">
    <% articles.articles.forEach(function(article){ %>
        <li>
            <a href="/<%= article.id %>">
                <div class="thumbnailWrapper">
                    <img src="<%= article.urlToImage %>" alt="<%= article.title %>">
                </div>
                <h1>
                    <%= article.title %>
                </h1>
                <h2>
                    <%= article.author %>
                </h2>
                <p class="date">
                    <%= article.date %>
                </p>
                <p class="articleIntro">
                    <%= article.quote %>. <span class="readMore">read more..</span>
                </p>
                <p class="articleTag">
                    article
                </p>
            </a>
        </li>
    <% }); %>
</ul>
