<ul class="articleList">
    <% articles.articles.forEach(function(article){ %>
        <li>
            <a href="">
                <div class="thumbnailWrapper">
                    <img src="<%= article.urlToImage %>" alt="<%= article.title %>">
                </div>
                <h1>
                    <%= article.title %>
                </h1>
            </a>
        </li>
    <% }); %>
</ul>

